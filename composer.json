{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "barryvdh/laravel-translation-manager": "^0.5.0",
        "cviebrock/eloquent-sluggable": "7.0.*",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "froiden/envato": "^1.0",
        "froiden/laravel-installer": "^1.6",
        "froiden/laravel-rest-api": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.3",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.1",
        "laravel/nexmo-notification-channel": "^2.2",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0.0",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "~3.1.0",
        "macellan/laravel-zip": "^1.0",
        "macsidigital/laravel-zoom": "^4.1",
        "nexmo/laravel": "^2.1",
        "nwidart/laravel-modules": "^6.0",
        "php-http/guzzle6-adapter": "^2.0",
        "swiftmailer/swiftmailer": "6.0.*",
        "trebol/entrust": "^1.0",
        "tymon/jwt-auth": "^1.0.0",
        "yajra/laravel-datatables-buttons": "~4.6",
        "yajra/laravel-datatables-html": "~4.4",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "start.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
